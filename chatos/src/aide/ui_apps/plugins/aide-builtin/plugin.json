{
  "manifestVersion": 1,
  "id": "com.leeoohoo.aideui.builtin",
  "name": "AIDE Built-in Apps",
  "version": "0.1.1",
  "description": "内置应用集合（CLI 等）",
  "apps": [
    {
      "id": "cli",
      "name": "AIDE 引擎",
      "description": "内置 CLI UI（会话 / 文件 / 轨迹 / 灵动岛）",
      "entry": {
        "type": "module",
        "path": "cli/dist/index.mjs",
        "compact": { "type": "module", "path": "cli/compact.mjs" }
      },
      "ai": { "mcpServers": true, "prompts": true }
    }
  ]
}
