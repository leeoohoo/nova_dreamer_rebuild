{
  "name": "@leeoohoo/aide",
  "version": "0.1.1",
  "description": "Terminal chat client for multiple AI models (CLI + Electron UI).",
  "license": "MIT",
  "bin": {
    "aide": "dist/cli.js",
    "aideui": "bin/aideui.js"
  },
  "type": "module",
  "files": [
    "bin/**/*",
    "dist/**/*",
    "shared/**/*",
    "subagents/**/*",
    "mcp_servers/**/*",
    "electron/**/*",
    "ui_apps/**/*",
    "apps/ui/dist/**/*",
    "build_resources/**/*",
    "mcp_config_restore.json",
    "mcp_config_update.json",
    "README.md"
  ],
  "scripts": {
    "start": "node src/cli.js",
    "chat": "node src/cli.js chat",
    "models": "node src/cli.js models",
    "test": "node --test",
    "engine:build": "node scripts/build-engine.js",
    "validate:defaults": "node scripts/validate-defaults.js",
    "prepack": "node scripts/build-engine.js --skip-if-present && node scripts/build-ui.js --release --skip-if-present && node scripts/build-cli-ui-plugin.js --release --skip-if-present",
    "ui:build": "node scripts/build-ui.js && node scripts/build-cli-ui-plugin.js",
    "ui": "node scripts/build-ui.js --skip-if-present && node scripts/build-cli-ui-plugin.js --skip-if-present && electron electron/main.js"
  },
  "dependencies": {
    "../common": "^0.1.1",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.21.2",
    "esbuild": "0.25.0",
    "highlight.js": "^11.11.1",
    "ink": "^5.0.1",
    "mermaid": "^10.9.1",
    "openai": "^6.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sql.js": "^1.12.0",
    "yaml": "^2.7.0",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "chrome-devtools-mcp": "^0.12.1",
    "electron": "38.0.0-beta.6"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
